<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="stackPane" alignment="TOP_LEFT" maxHeight="450" maxWidth="800" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="polis.PolisCompanion">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Button fx:id="buttonResidence" mnemonicParsing="false" onAction="#residenceSelected" prefHeight="40.0" prefWidth="50.0" />
      <Button fx:id="buttonIndustry" mnemonicParsing="false" onAction="#industrySelected" prefHeight="40.0" prefWidth="50.0" translateX="75.0" />
      <Button fx:id="buttonCommerce" mnemonicParsing="false" onAction="#commerceSelected" prefHeight="40.0" prefWidth="50.0" translateX="150.0" />
      <Button fx:id="buttonRoad" mnemonicParsing="false" onAction="#roadSelected" prefHeight="40.0" prefWidth="50.0" translateY="50.0" />
      <Button fx:id="buttonBulldozer" mnemonicParsing="false" onAction="#bulldozerSelected" prefHeight="40.0" prefWidth="50.0" translateX="75.0" translateY="50.0" />
      <Button fx:id="buttonSelection" mnemonicParsing="false" onAction="#selectionSelected" prefHeight="40.0" prefWidth="50.0" translateY="100.0" />
      <Button fx:id="buttonSimulator" mnemonicParsing="false" onAction="#simulatorSelected" prefHeight="40.0" prefWidth="80.0" StackPane.alignment="BOTTOM_LEFT" />
   </children>
</StackPane>
